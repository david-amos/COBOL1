IF I-COLOR = "RED"
	DISPLAY "WOW".
	
IF I-AGE > 18
	IF I-COLOR = "BROWN'
		ADD 1 TO C-OLDBROWN-CTR. <-- ENDS BOTH
		
		
IF I-AGE > 18
	IF I-COLOR = "BROWN"
		ADD 1 TO CTR
	ELSE
		NEXT SENTENCE
ELSE
	DISPLAY "ACT LIKE A CHILD'
	ADD 1 TO KID-CTR.
	
	
	
	
IF DAY="MONDAY" AND TIME=12
	DISPLAY "LUNCHTIME!".
	
	
	
SPELL OUT NOT, OR, AND



IF I-AGE < 10 OR > 20
	DISPLAY "OK".
	
	
	
EVALUATE IDENTIFIER
	WHEN CONDITION 1
	WHEN OTHER SADFJASOD
END-EVALUATE

EVALUATE AVERAGE //INCLUSIVE
	WHEN 93 THRU 100
		MOVE ----
	WHEN 85 THRU 92
		PERFRMOSADFJLSK.
	
	
	
	
	
EVALUATE SWITCH = 'ON'
	WHEN TRUE
		PERFORM ASFJDAS;F
	WHEN FALSE
		PERFORM SAFL;JL;SKAD.




