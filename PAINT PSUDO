VARIABLE DECLARATIONS
	ALFANUMERIC I-PAINT-EST-NO, MORE-RECS = "YES"
	NUMERIC PAINT-DATE: I-PAINT-YY, I-PAINT-MM, I-PAINT-DD/ I-PAINT-WALL-SQ-FT, I-PAINT-DOOR-SQ-FT, I-PAINT-PRICE-GAL, 
		C-TOTAL-SQ-FT, C-GAL, C-PAINT-EST, C-HOURS, C-LABOR-EST, C-TOTAL-EST, C-PCTR
		C-GT-CTR = 0, C-GT-GAL = 0, C-GT-PAINT-EST = 0, C-GT-LABOR-EST = 0, C-GT-TOTAL-EST = 0
		O-EST-DATE, O-WALL-SQ-FT, O-DOOR-SQ-FT, O-TOTAL-SQ-FT, O-PAINT-PRICE-GAL, O-GAL, O-PAINT-EST, 0-LABOR-EST, O-TOTAL-EST, O-PCTR
		O-GT-CTR, O-GT-GAL, O-GT-PAIN-EST, O-GT-LABOR-EST, O-GT-TOTAL-EST
		
		
MAIN
	PERFORM INIT
	PERFORM MAINLINE
		UNTIL MORE-RECS = "NO"
	PERFORM CLOSING
	
MAINLINE
	PERFORM CALCS
	PERFORM OUTPUT
	PERFORM READ
	
INIT
	OPEN INPUT PAINTEST.DATE
	OPEN OUTPUT PJOBEST.PRT
	REQUEST DATE
	GET DATE
	FORMAT DATE
	CALL READ
	CALL HEADINGS
	
READ
	READ FILE
	IF END OF FILE
		MORE-RECS = "NO"

HEADINGS
	C-PCTR ++
	O-PCTR = C-PCTR
	PRINT COMPANY TITLE
	PRINT BLANK
	PRINT COLLUMN HEADINGS 1
	PRINT COLLUMN HEADINGS 2
	PRINT BLANK
	
CALCS
	C-TOTAL-SQ-FT = I-PAINT-WALL-SQ-FT - I-PAINT-DOOR-SQ-FT
	C-GAL = C-TOTAL-SQ-FT / 115
	C-PAINT-EST = I-GAL * I-PAINT-PRICE-GAL
	C-LABOR-EST = 1-GAL * 3 * 23.55
	C-TOTAL-EST = C-PAINT-EST + C-LABOR-EST
	
	C-GT-CTR ++
	C-GT-GAL += C-GAL
	C-GT-PAINT-EST += C-PAINT-EST
	C-GT-LABOR-EST += C-LABOR-EST
	C-GT-TOTAL-EST += C-TOTAL-EST
	
OUTPUT
	FORMAT VARIABLES
	
	PRINT DETAIL LINE
	IF EOF
		CALL HEADINGS
	
CLOSING
	FORMAT VARIABLES
	PRINT BLANK
	PRINT BLANK
	PRINT GT LINE
	CLOSE INPUT
	CLOSE OUTPUT

	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	